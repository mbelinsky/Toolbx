<%= render 'admin/header' %>
<%= render 'shared/flashes' %>

<ul class="items full-width">
    <li class="link">
        <%= link_to new_admin_roundup_path do %><span class="add">Add Roundup</span><% end %>
    </li>

    <% @roundups.each do |roundup| %>
        <%= render 'admin/roundups/roundup', roundup: roundup, path: edit_admin_roundup_path(roundup) %>
    <% end %>
</ul>

<div id="infinite-indicator" class="group"></div>
